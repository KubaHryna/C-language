#include <stdio.h>
#include <stdlib.h>
void wypisz(unsigned int n,unsigned int m,int **t){
for(int i=0;i<n;i++)
    {
        for(int j=0;i<m;i++)
            {
                printf(" %d ", t[i][j]);
            }
            printf("\n");
    }
}
void zerujaca(unsigned int n,unsigned int m,int t[][m]){
int i,j;
for(i=0;i<n;i++){
    for(j=0;j<m;j++){
        t[i][j] = 0;
    }
}
}
int main(){
    int tab[2][3]={(4,7,6),(5,2,9)};
    wypisz(2,3,tab);


return 0;
}
